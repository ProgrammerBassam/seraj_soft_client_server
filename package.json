{
  "name": "seraj_soft_client_server",
  "version": "1.0.3",
  "description": "Seraj Soft API",
  "main": "main.js",
  "scripts": {
    "start": "node server/server.js",
    "electron-start": "concurrently \"npm run start\" \"wait-on http://localhost:65531 && electron .\"",
    "electron-start-prod": "concurrently \"npm run start\" \"wait-on http://localhost:65531 && electron . --no-console --main=main.prod.js\"",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.serajSoft.clientServer",
    "files": [
      "public/**/*",
      "main.prod.js",
      "server/server.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "keywords": [],
  "author": "Seraj Soft",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^8.22.0",
    "@sentry/profiling-node": "^8.22.0",
    "@whiskeysockets/baileys": "github:WhiskeySockets/Baileys",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.7.4",
    "connect-timeout": "^1.9.0",
    "cron": "^3.1.7",
    "electron-updater": "^6.2.1",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.6.3",
    "ip": "^2.0.1",
    "morgan": "^1.10.0",
    "network": "^0.7.0",
    "nexe": "^4.0.0-rc.6",
    "node-cron": "^3.0.3",
    "npm-check-updates": "^17.0.0",
    "open": "^10.1.0",
    "oracledb": "^6.6.0",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "superagent": "^9.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "wait-on": "^7.2.0"
  }
}